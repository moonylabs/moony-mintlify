{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "maple",
  "name": "Maple Starter Kit",
  "colors": {
    "primary": "#117866",
    "light": "#4ADE80",
    "dark": "#166534"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": [
          "docs/welcome",
          "docs/quickstart",
          "docs/development"
        ]
      },
      {
        "group": "Essentials",
        "pages": [
          "docs/markdown-syntax",
          "docs/code-blocks",
          "docs/images-and-embeds",
          "docs/settings",
          "docs/navigation",
          "docs/reusable-snippets"
        ]
      },
      {
        "group": "API Documentation",
        "pages": [
          "docs/api-reference/introduction"
        ]
      },
      {
        "group": "Endpoint Examples",
        "pages": [
          "docs/api-reference/get-plants",
          "docs/api-reference/create-plant",
          "docs/api-reference/delete-plant"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ],
      "languages": [
        {
          "language": "en",
          "default": true
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
